{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "functions": {
        "api/flask.py": {
            "runtime": "python3.11",
            "maxDuration": 10
        }
    },
    "buildCommand": "pip install -r requirements.txt",
    "outputDirectory": "api",
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        }
    ]
}